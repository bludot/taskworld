{
  "name": "server",
  "version": "0.0.0",
  "types": "src/index.d.ts",
  "scripts": {
    "watch": "tsc -w -p ./src",
    "build:clean": "rm -rf dist",
    "build:server": "tsc",
    "build": "npm run build:clean",
    "watch-node": "nodemon --legacy-watch",
    "docker-debug": "docker-compose up",
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "main": "./dist/index.js",
  "dependencies": {
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "intersects": "^2.7.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
